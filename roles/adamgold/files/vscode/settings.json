{
	"workbench.colorTheme": "Monokai Dark Soda Modified",
	"editor.fontFamily": "Fira Code, Monaco, 'Courier New', monospace",
	"editor.cursorBlinking": "phase",
	"python.linting.enabled": true,
	"editor.scrollBeyondLastLine": false,
	"monokaiPro.fileIconsMonochrome": true,
	"git.enableSmartCommit": true,
	"editor.renderControlCharacters": true,
	"editor.formatOnPaste": true,
	"[python]": {
		"editor.formatOnPaste": false
	},
	"editor.stablePeek": true,
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"editor.lineNumbers": "relative",
	"terminal.integrated.shell.osx": "zsh",
	"terminal.integrated.rendererType": "dom",
	"editor.fontLigatures": true,
	"workbench.startupEditor": "none",
	"editor.multiCursorModifier": "ctrlCmd",
	"python.linting.banditEnabled": true,
	"editor.folding": false,
	"editor.cursorStyle": "line-thin",
	"breadcrumbs.enabled": true,
	"workbench.editor.highlightModifiedTabs": true,
	"window.restoreWindows": "all",
	"workbench.colorCustomizations": {
		"terminalCursor.foreground": "#00ffaa",
		"terminal.foreground": "#839496",
		"terminal.background": "#002833"
	},
	"terminal.integrated.cursorBlinking": true,
	"python.testing.autoTestDiscoverOnSaveEnabled": false,
	"javascript.suggest.autoImports": false,
	"python.linting.mypyEnabled": true,
	"editor.formatOnSave": true,
	"python.formatting.provider": "black",
	"terminal.integrated.fontFamily": "MesloLGS NF",
	"python.venvPath": "~/.local/share/virtualenvs",
	"python.pythonPath": "~/.pyenv/versions/3.8.2/bin/python3.8",
	"python.venvFolders": [
		"~/.local/share/virtualenvs"
	],
	"tabnine.experimentalAutoImports": true,
	"python.formatting.blackPath": "~/.local/bin/black",
	"python.linting.banditPath": "~/.local/bin/bandit",
	"python.linting.mypyPath": "~/.local/bin/mypy",
	"files.insertFinalNewline": true,
	"python.terminal.activateEnvironment": false,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"terminal.integrated.tabs.enabled": false,
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"terminal.integrated.scrollback": 3000,
	"python.defaultInterpreterPath": "~/.pyenv/versions/3.8.2/bin/python3.8"
}
